{
  "name": "vtb-tetris",
  "version": "1.1.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pre-build": "mkdir -p ./dist ./dist/server ./dist/client ./dist/common && cp -r ./src/common ./dist/",
    "build": "npm run srv-build && npm run client-build",
    "client-dev": "webpack-dev-server --mode development --open",
    "client-build": "npm run pre-build && webpack ./src/client/index.js ",
    "srv-dev": "babel-watch -w src src/server/App.js",
    "srv-build": "npm run pre-build && babel src/server --out-dir dist/server",
    "test-all": "npm run test-server && npm run test-client",
    "test-server": "mocha --recursive --require babel-core/register --reporter spec \"test/1_server/\"",
    "test-client": "mocha --recursive --require babel-core/register -r jsdom-global/register --reporter spec \"test/2_client/\"",
    "coverage": "nyc npm run test-all && nyc report --reporter=lcov",
    "predeploy": "npm run build",
    "deploy": "echo 'not available'",
    "serve": "node dist/server/App.js",
    "clean": "rm -rf dist && rm -rf .nyc_output",
    "clean-all": "npm run clean && rm -rf node_modules"
  },
  "keywords": [],
  "author": "ONIXX",
  "license": "",
  "dependencies": {
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "killable": "^1.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "redux": "^3.7.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "dotenv": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-watch": "^2.0.8",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "jsdom": "11.6.2",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^11.9.0",
    "style-loader": "^0.20.3"
  }
}
